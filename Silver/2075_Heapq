#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>


// 꼭 모든 데이터를 저장할 자료 구조가 필요한 것은 아니다!!!!!!!!!!!!
// 현재 내가 원하는 값을 출력하기 위해 필요한 명확한 자료구조의 형태를 찾자!!!!!!!!!!
/*
5
12 7 9 15 5
13 8 11 19 6
21 10 26 31 16
48 14 28 35 25
52 20 32 41 49
*/

int main(void)
{
	int* arr;
	int arr_N, num, left, right, child, tmp;
	int top = 0;
	int curr;
	scanf("%d", &arr_N);
	int total = arr_N * arr_N;
	arr = (int*)malloc(sizeof(int) * (arr_N + 1));

	for (int i = 1; i <= arr_N; i++)
	{
		scanf("%d", &num);

		top++;
		curr = top;
		while (curr > 1 && num < arr[curr / 2])
		{
			arr[curr] = arr[curr / 2];
			curr = curr / 2;
		}

		arr[curr] = num;
	}

	for (int i = arr_N + 1; i <= total; i++)
	{
		scanf("%d", &num);

		if (num > arr[1])
		{
			arr[1] = num;
			curr = 1;
			while (1)
			{
				left = curr * 2;
				right = curr * 2 + 1;

				if (left > arr_N)
					break;

				child = left;
				if (right <= arr_N && arr[child] > arr[right])
				{
					child = right;
				}
				if (arr[curr] < arr[child])
				{
					break;
				}

				tmp = arr[child];
				arr[child] = arr[curr];
				arr[curr] = tmp;
				curr = child;
			}
		}

   return 0;
	}

	printf("%d", arr[1]);

	return 0;
}
